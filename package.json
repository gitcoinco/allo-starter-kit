    "@testing-library/react-hooks": "^8.0.1",
    "@turbo/gen": "^1.13.4",
    "@types/node": "^20.14.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.0.5",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "glob": "^11.0.0",
    "happy-dom": "^14.12.3",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "serve": "^14.2.1",
    "storybook": "^8.3.5",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "wagmi": "^2.12.2"
  },
  "peerDependencies": {
    "@rainbow-me/rainbowkit": "^2.1.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "wagmi": "^2.12.2"
  },
  "dependencies": {
    "@allo-team/allo-v2-sdk": "^1.0.78",
    "@gitcoin/gitcoin-chain-data": "^1.0.26",
