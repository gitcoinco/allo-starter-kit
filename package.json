{
  "name": "@allo-team/kit",
  "version": "0.1.12",
  "private": false,
  "main": "./dist/index.mjs",
  "readme": "../../README.md",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/gitcoinco/allo-starter-kit.git"
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./button": "./src/ui/button.tsx",
    "./applications": "./src/applications/index.ts",
    "./projects": "./src/projects/index.ts",
    "./rounds": "./src/rounds/index.ts",
    "./api": "./src/api/index.ts",
    "./wagmi": "./src/wagmi/index.ts",
    "./connect-button": "./src/connect-button.tsx"
  },
  "scripts": {
    "build": "tsup",
    "build2": "tsup && tsc --emitDeclarationOnly --declaration --jsx react-jsx",
    "postbuild": "node fix-mjs-imports.js",
    "lint": "eslint src/ ./stories/*.stories.tsx",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "build:storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test:storybook": "pnpm test-storybook",
    "build:docs": "storybook build --docs",
    "preview-storybook": "serve storybook-static"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.5",
    "@rainbow-me/rainbowkit": "^2.1.4",
